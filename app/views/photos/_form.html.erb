<%= form_with model: [@album, @album.photos.build] do |form|%>
        <p class = "my-1"style="margin-left: 200px;">
        <%=form.file_field :image ,multiple: :true%><br>
        </p>
        <%=form.submit 'Add Photo',class: "inline-block align-middle text-center select-none border font-normal whitespace-no-wrap rounded py-1 px-3 leading-normal no-underline bg-gray-600 text-white hover:bg-gray-700 my-2"%>
<%end%>